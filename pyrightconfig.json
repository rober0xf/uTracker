{
    "include": [
        "app"
    ],
    "exclude": [
        "**/node_modules",
        "**/__pycache__",
        "**/.venv",
        "**/.git",
        "**/.idea",
        "**/migrations"
    ],
    "venvPath": ".",
    "venv": ".venv",
    "reportMissingImports": true,
    "reportMissingTypeStubs": false,
    "reportArgumentType": "none",
    "pythonVersion": "3.13",
    "autoImportCompletions": false,
    "typeCheckingMode": "basic",
    "reportAny": "none"
}